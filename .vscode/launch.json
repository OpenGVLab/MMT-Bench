    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "main",
            "type": "python",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--data", "MMT-Bench_ALL",
                "--model", "llava_v1.5_7b",
                "--work-dir", "work_dirs/mmtbench"
            ]
        },
    ]
}